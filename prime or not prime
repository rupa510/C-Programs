#include<stdio.h>
void main()
{
    int i,n,count=0;
        printf("enter n:");
        scanf("%d",&n);
        for(i=1;i<=n;i++)
        {
            if(n%i==0)
            {
                count++;
            }
        }
        if(count==2)
        {
            printf("given number is prime %d");
        }
        else{
            printf("%d is not a prime");
        }
    }
